{
  "name": "reselector",
  "version": "0.8.1",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:lttb/reselector.git"
  },
  "author": "",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean",
    "build": "BABEL_ENV=production ./node_modules/.bin/babel src --ignore '**/tests/**' --out-dir lib",
    "postbuild": "npm run cp:meta",
    "cp:meta": "copyfiles -e '**/tests/**' package.json README.md 'src/.*' -a -f lib",
    "test": "BABEL_ENV=test jest",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "./node_modules/.bin/eslint .",
    "preversion": "npm run lint && npm test",
    "postversion": "git push --follow-tags && npm run build && npm publish lib",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@lttb/eslint-config-default": "github:lttb/configs#js",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^22.2.2",
    "copyfiles": "^1.2.0",
    "coveralls": "^3.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.13.1",
    "husky": "^0.14.3",
    "jest": "^22.0.3",
    "lint-staged": "^6.1.0",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-syntax-async-generators": "^7.0.0-beta.40",
    "@babel/plugin-syntax-class-properties": "^7.0.0-beta.40",
    "@babel/plugin-syntax-decorators": "^7.0.0-beta.40",
    "@babel/plugin-syntax-do-expressions": "^7.0.0-beta.40",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.40",
    "@babel/plugin-syntax-export-default-from": "^7.0.0-beta.40",
    "@babel/plugin-syntax-export-namespace-from": "^7.0.0-beta.40",
    "@babel/plugin-syntax-flow": "^7.0.0-beta.40",
    "@babel/plugin-syntax-function-bind": "^7.0.0-beta.40",
    "@babel/plugin-syntax-function-sent": "^7.0.0-beta.40",
    "@babel/plugin-syntax-import-meta": "^7.0.0-beta.40",
    "@babel/plugin-syntax-jsx": "^7.0.0-beta.40",
    "@babel/plugin-syntax-nullish-coalescing-operator": "^7.0.0-beta.40",
    "@babel/plugin-syntax-numeric-separator": "^7.0.0-beta.40",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0-beta.40",
    "@babel/plugin-syntax-optional-catch-binding": "^7.0.0-beta.40",
    "@babel/plugin-syntax-optional-chaining": "^7.0.0-beta.40",
    "@babel/plugin-syntax-pipeline-operator": "^7.0.0-beta.40",
    "@babel/plugin-syntax-throw-expressions": "^7.0.0-beta.40",
    "@babel/plugin-syntax-typescript": "^7.0.0-beta.40",
    "@babel/template": "^7.0.0-beta.40",
    "cosmiconfig": "^4.0.0",
    "string-hash": "^1.1.3"
  }
}
